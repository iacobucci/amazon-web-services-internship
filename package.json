    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0"
  }
