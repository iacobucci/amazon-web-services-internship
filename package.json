{
  "name": "express-aws",
  "type": "module",
  "version": "1.0.0",
  "description": "Aws S3 with express",
  "main": "src/main.ts",
  "scripts": {
    "dev": "sh util/tunnel.sh & tsc -w & nodemon -q -w dist dist/main.js",
    "build": "rm -rf dist/ ; mkdir -p dist; cp src/index.html dist/index.html ; tsc; esbuild dist/main.js --bundle --outfile=dist/bundle.cjs --platform=node --target=es2022",
    "server": "node dist/bundle.cjs"
  },
  "author": "iacobucci",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.301.0",
    "@aws-sdk/types": "^3.296.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/node": "18.15.11",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "esbuild-loader": "3.0.1",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.22",
    "ts-loader": "9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.3",
    "uuid": "^9.0.0",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1"
  }
}
